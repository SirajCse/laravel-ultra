{
  "name": "sirajcse/laravel-ultra",
  "description": "The most advanced AI-powered UI component suite for Laravel",
  "keywords": ["laravel", "ui", "components", "ai", "realtime", "vue", "react", "table", "form", "modal"],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LaravelUltra\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelUltra\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0",
    "inertiajs/inertia-laravel": "^1.0",
    "openai-php/client": "^0.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0",
    "mockery/mockery": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelUltra\\Core\\UltraServiceProvider"
      ],
      "aliases": {
        "Ultra": "LaravelUltra\\Core\\UltraFacade"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}