{
  "name": "sirajcse/laravel-ultra",
  "description": "The most advanced AI-powered UI component suite for Laravel",
  "keywords": ["laravel", "ui", "components", "ai", "table", "form", "modal"],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LaravelUltra\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelUltra\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1|^8.2",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0|^12.0",
    "orchestra/testbench": "^8.0|^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelUltra\\Core\\UltraServiceProvider"
      ],
      "aliases": {
        "Ultra": "LaravelUltra\\Core\\UltraFacade"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}